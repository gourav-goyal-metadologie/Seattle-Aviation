    <rules>
        <fullName>QPMS__CR Task Due Date Change Notification</fullName>
        <actions>
            <name>QPMS__CR_Task_Due_Date_Change_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>CR Task Due Date Change Notification</description>
        <formula>AND( QPMS__Due_Date__c != PRIORVALUE(QPMS__Due_Date__c), QPMS__Change_Management__c  &lt;&gt; NULL, OR( ISPICKVAL( QPMS__Change_Management__r.QPMS__Status__c , &apos;Change Workflow&apos;) , ISPICKVAL( QPMS__Change_Management__r.QPMS__Status__c , &apos;Document Change Order&apos;) ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>QPMS__Email when owner changed in all modules</fullName>
        <actions>
            <name>QPMS__sends_an_email_when_owner_changed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>email when owner changed.</description>
        <formula>AND(NOT(ISNULL( Id )),OwnerId != PRIORVALUE(OwnerId))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>QPMS__NC Task Status Update Pending Approval</fullName>
        <actions>
            <name>QPMS__NC_Task_Pending_Approval_notification</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>QPMS__NCTask_Status_Update_as_Approval_Pending</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>QPMS__General_Task__c.QPMS__Approval_Required__c</field>
