    <rules>
        <fullName>QPMS__On reassign</fullName>
        <actions>
            <name>QPMS__On_reassign_task_owner</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(OwnerId != PRIORVALUE(OwnerId ), QPMS__Record_Type_Name__c ==&apos;EffectivenessReview&apos;, ISPICKVAL( QPMS__Status__c , &apos;Pending Approval&apos;)  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>QPMS__On reassign Change Management Task</fullName>
        <actions>
            <name>QPMS__On_Task_Reassign_Change_Management</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(OwnerId != PRIORVALUE(OwnerId ),  ISNULL( QPMS__Change_Management__c ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
