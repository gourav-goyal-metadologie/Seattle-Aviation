    <validationRules>
        <fullName>inscor__Cancelled_Payble_Not_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( inscor__Consignor_Payable__c ) &amp;&amp;   ISPICKVAL(inscor__Consignor_Payable__r.inscor__Status__c , &apos;Cancelled&apos;)</errorConditionFormula>
        <errorMessage>Can not add additional Adjustments to Cancelled Payable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__GL_Account_Both_or_None</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR( 
	AND( ISBLANK(inscor__GL_Account_1__c), ISBLANK(inscor__GL_Account_2__c), ISBLANK(inscor__Amount_1__c), ISBLANK(inscor__Amount_2__c)), 
	AND( NOT(ISBLANK(inscor__GL_Account_1__c)), NOT(ISBLANK(inscor__GL_Account_2__c)), NOT(ISBLANK(inscor__Amount_1__c)), NOT(ISBLANK(inscor__Amount_2__c)))
))</errorConditionFormula>
        <errorMessage>Either fill both GL Account 1 and GL Account 2 with respective Amount fields or leave all these fields blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__GL_Amount_Match</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(inscor__Amount_1__c)), NOT(ISBLANK(inscor__Amount_2__c)), ABS(inscor__Amount_1__c) != ABS(inscor__Amount_2__c))</errorConditionFormula>
        <errorMessage>GL Account 1 Amount and GL Account 2 Amount must be same. Though values can have positive or negative sign.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
