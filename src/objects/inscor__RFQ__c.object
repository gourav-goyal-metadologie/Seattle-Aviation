        <columns>inscor__Status__c</columns>
        <columns>inscor__Urgency__c</columns>
        <columns>inscor__RFQ_Total_Value__c</columns>
        <columns>inscor__Assigned_To__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>inscor__Status__c</field>
            <operation>notEqual</operation>
            <value>Complete,Cancelled</value>
        </filters>
        <label>All Open RFQs</label>
    </listViews>
    <nameField>
        <displayFormat>RFQ-{0}</displayFormat>
        <label>RFQ Number</label>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>RFQs</pluralLabel>
    <searchLayouts>
        <searchResultsAdditionalFields>inscor__Vendor__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>inscor__RFQ_Total_Value__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>inscor__Status__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>inscor__Urgency__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>inscor__Assigned_To__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATED_DATE</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>inscor__Business_Category_blank_Auto_Update</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Business_Category_Auto_Update__c == true &amp;&amp; NOT(ISBLANK(TEXT(inscor__Business_Category__c)))</errorConditionFormula>
        <errorMessage>Auto Update Cannot be true when Business Category Selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cancelled_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISCHANGED(inscor__Status__c),
    TEXT(PRIORVALUE(inscor__Status__c)) = &quot;Cancelled&quot;
)</errorConditionFormula>
        <errorMessage>Once cancelled, the RFQ cannot be reopened.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Company_field_required</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Company__c = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>inscor__Company__c</errorDisplayField>
        <errorMessage>Company is a required field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Need_by_cannot_be_less_than_today</fullName>
        <active>false</active>
        <errorConditionFormula>CreatedDate   &lt; now()</errorConditionFormula>
        <errorMessage>Need by date cannot be less than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Status_cannot_be_previous</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;In Progress&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Sent to Vendor&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Complete&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;Requested&quot;)
   )
,
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Sent to Vendor&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Complete&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;In Progress&quot;)
   )
,
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Complete&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;Sent to Vendor&quot;)
   )
)</errorConditionFormula>
        <errorMessage>Status cannot be changed to a previous status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Vendor_field_required</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Vendor__c  = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>inscor__Vendor__c</errorDisplayField>
        <errorMessage>Vendor is a required field</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
