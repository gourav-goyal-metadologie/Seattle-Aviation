    <validationRules>
        <fullName>inscor__Block_Misc_Charge_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(inscor__Type__c, &apos;Misc Charge&apos;), inscor__Misc_Charge_Allowed__c = false)</errorConditionFormula>
        <errorMessage>Can&apos;t create BOM of type Misc Charge. Please use &quot;Add Misc Charge&quot; button at Task page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Hours_Billed_Required_If_Type_Labor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(inscor__Type__c , &quot;Labor&quot;) ,ISNULL( inscor__Hours__c ))</errorConditionFormula>
        <errorDisplayField>inscor__Hours__c</errorDisplayField>
        <errorMessage>Please Enter Hours Billed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Material_Price_Required_if_Type_Material</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(inscor__Type__c , &quot;Material&quot;) , ISBLANK(inscor__Material_Price__c))</errorConditionFormula>
        <errorDisplayField>inscor__Material_Price__c</errorDisplayField>
        <errorMessage>Please Select a Material Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Part_Number_Required_if_Type_Material</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(inscor__Type__c , &quot;Material&quot;) , ISBLANK(  inscor__Product__c ))</errorConditionFormula>
        <errorDisplayField>inscor__Product__c</errorDisplayField>
        <errorMessage>Please Select a Part Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Prevent_Reserved_from_surpassing_Request</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Quantity_Reserved__c &gt; inscor__Quantity_Requested__c</errorConditionFormula>
        <errorMessage>The Quantity Reserved cannot be greater than the Quantity Requested</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Qty_Should_Greater_then_Issue_and_Return</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Product__r.inscor__Non_Inventory_Item__c == false &amp;&amp;  inscor__Quantity_Requested__c &lt; ( BLANKVALUE( inscor__Quantity_Issued__c, 0) -  BLANKVALUE(  inscor__Quantity_Returned__c, 0)  )</errorConditionFormula>
        <errorMessage>Quantity Requested cannot be less than the Quantity Issued minus the Quantity Returned. Please process a material return before reducing the Quantity Requested.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Skill_Required_If_Type_Labor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(inscor__Type__c , &quot;Labor&quot;) ,ISBLANK( inscor__Skill__c  ))</errorConditionFormula>
        <errorDisplayField>inscor__Skill__c</errorDisplayField>
        <errorMessage>Please Enter a Skill.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
