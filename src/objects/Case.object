    <validationRules>
        <fullName>QPMS__Defect_Statement</fullName>
        <active>true</active>
        <description>Defect Statement Should not be left blank</description>
        <errorConditionFormula>LEN( QPMS__Defect_Statement__c ) &lt;= 0</errorConditionFormula>
        <errorDisplayField>QPMS__Defect_Statement__c</errorDisplayField>
        <errorMessage>Defect Statement Should not be left empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Occurance_Date_not_future</fullName>
        <active>true</active>
        <errorConditionFormula>QPMS__Occurrence_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>QPMS__Occurrence_Date__c</errorDisplayField>
        <errorMessage>Occurrence Date cannot be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Occurrence_Date</fullName>
        <active>true</active>
        <description>Occurrence Date cannot be greater than the Reported Date</description>
        <errorConditionFormula>QPMS__Occurrence_Date__c &gt; QPMS__Reported_Date__c</errorConditionFormula>
        <errorDisplayField>QPMS__Occurrence_Date__c</errorDisplayField>
        <errorMessage>Occurrence Date cannot be greater than the Reported Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Process_CaseType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( QPMS__Case_Type__c , &apos;Process&apos;), ISBLANK( QPMS__Process__c ) )</errorConditionFormula>
        <errorDisplayField>QPMS__Process__c</errorDisplayField>
        <errorMessage>Process should not be left blank when Case Type is Process</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Process_Defect</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( QPMS__Process__c ) ), ISBLANK( QPMS__Process_Defect__c ))</errorConditionFormula>
        <errorMessage>Process Defect should not be left blank when Process is not blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__ProductDefectOrProcessDefect</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( QPMS__Process_Defect__c ) ),NOT( ISBLANK( QPMS__Product_Defect__c ) ))</errorConditionFormula>
        <errorMessage>Have to select only one at a time either &quot;Product Defect&quot; or &quot;Process Defect&quot; based on &quot;Product&quot; or &quot;Process&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__ProductOrProcess</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( QPMS__Process__c ) ),NOT( ISBLANK( QPMS__Product_Family__c ) ))</errorConditionFormula>
        <errorMessage>Have to select only one at a time either &quot;Product&quot; or &quot;Process&quot; based on &quot;Case Type&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Product_CaseType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( QPMS__Case_Type__c , &apos;Product&apos;), ISBLANK( QPMS__Product_Family__c ) )</errorConditionFormula>
        <errorDisplayField>QPMS__Product_Family__c</errorDisplayField>
        <errorMessage>Product should not be left blank when Case Type is Product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Product_Defect</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( QPMS__Product_Family__c ) ), ISBLANK( QPMS__Product_Defect__c ))</errorConditionFormula>
        <errorDisplayField>QPMS__Product_Defect__c</errorDisplayField>
        <errorMessage>Product Defect should not be left blank when Product is not blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Reported_Date</fullName>
        <active>true</active>
        <description>Reported Date should not be a future date</description>
        <errorConditionFormula>QPMS__Reported_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>QPMS__Reported_Date__c</errorDisplayField>
        <errorMessage>Reported Date should not be a future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Reported_Date_cannot_be_before_Occurance</fullName>
        <active>true</active>
        <errorConditionFormula>QPMS__Reported_Date__c &lt; QPMS__Occurrence_Date__c</errorConditionFormula>
        <errorDisplayField>QPMS__Reported_Date__c</errorDisplayField>
        <errorMessage>Reported Date cannot be less than Occurrence Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QPMS__Reporter</fullName>
        <active>true</active>
        <description>Reporter Should not be left blank</description>
        <errorConditionFormula>ISBLANK( QPMS__Reporter__c )</errorConditionFormula>
        <errorDisplayField>QPMS__Reporter__c</errorDisplayField>
        <errorMessage>Reporter Should not be left blank</errorMessage>
    </validationRules>
