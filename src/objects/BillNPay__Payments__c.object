    <validationRules>
        <fullName>BillNPay__Canceled_Validation</fullName>
        <active>true</active>
        <description>Only records in Request Sent may be canceled</description>
        <errorConditionFormula>BillNPay__Cancelled__c = TRUE &amp;&amp;
 AND(
   !ISPICKVAL( BillNPay__Status__c ,&quot;New&quot;),
   !ISPICKVAL( BillNPay__Status__c ,&quot;In Process&quot;), 
   !ISPICKVAL( BillNPay__Status__c ,&quot;Request Sent&quot;),  
   !ISPICKVAL( BillNPay__Status__c ,&quot;Cancelled&quot;)  
   )</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>The Canceled Checkbox and Status are only available if the Payment was previously in &quot;New&quot;, &quot;In Process&quot; or &quot;Request Sent&quot; Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_ACH_Returned</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(BillNPay__Status__c,&apos;ACH Returned&apos;) &amp;&amp; BillNPay__Payment_Method__r.RecordType.DeveloperName = &quot;Card&quot; ||

AND
(
 ISPICKVAL(BillNPay__Status__c,&apos;ACH Returned&apos;), 
 NOT(CONTAINS(TEXT(PRIORVALUE(BillNPay__Status__c)),&apos;Approved/Paid&apos;))
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  ACH Returned must be for a Payment Method of record type Bank Account with a prior status of Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_ApprovedPaid</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
CONTAINS(TEXT(BillNPay__Status__c),&apos;Approved/Paid&apos;), TEXT(PRIORVALUE(BillNPay__Status__c))!=&apos;Request Sent&apos;
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  Approved/Paid must be preceded by Request Sent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_Charge_Back</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(TEXT(BillNPay__Status__c),&apos;Card Charge Back&apos;) &amp;&amp; BillNPay__Payment_Method__r.RecordType.DeveloperName = &quot;Bank Account&quot; || 

AND 
( 
 CONTAINS(TEXT(BillNPay__Status__c),&apos;Card Charge Back&apos;), 
 AND(
  NOT(CONTAINS(TEXT(PRIORVALUE(BillNPay__Status__c)),&apos;Approved&apos;)),
  NOT(CONTAINS(TEXT(PRIORVALUE(BillNPay__Status__c)),&apos;Charge Back&apos;)) 
  ) 
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  Charge Back status must be for a Payment Method of record type &apos;Card&apos; with a prior status of Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_Declined</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISPICKVAL(BillNPay__Status__c,&apos;Declined&apos;), TEXT(PRIORVALUE(BillNPay__Status__c))!=&apos;Request Sent&apos;
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  Declined must be preceded by Request Sent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_New</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp;

AND
(
ISPICKVAL(BillNPay__Status__c,&apos;New&apos;), 
TEXT(PRIORVALUE(BillNPay__Status__c)) &lt;&gt; &apos;Error&apos;
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  You may only change a status to New if prior status = Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_UserLookUp</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp;

AND
(
ISPICKVAL(BillNPay__Status__c,&apos;User Lookup&apos;), 
TEXT(PRIORVALUE(BillNPay__Status__c)) &lt;&gt; &apos;New&apos;
)</errorConditionFormula>
        <errorMessage>Invalid Status. You may only change a status to User Lookup if prior status = New</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Status_Validation_VoidRefund</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
CONTAINS(TEXT(BillNPay__Status__c),&apos;Void/Refund&apos;),
 TEXT(PRIORVALUE(BillNPay__Status__c))!=&apos;Request Sent&apos;,
 TEXT(PRIORVALUE(BillNPay__Status__c))!=&apos;Error&apos;,
 NOT(CONTAINS(TEXT(PRIORVALUE(BillNPay__Status__c)),&apos;Approved/Paid&apos;)),
 NOT(CONTAINS(TEXT(PRIORVALUE(BillNPay__Status__c)),&apos;Charge Back&apos;))
)</errorConditionFormula>
        <errorDisplayField>BillNPay__Status__c</errorDisplayField>
        <errorMessage>Invalid Status.  Void/Refund must be preceded by Request Sent, Approved/Paid, or Charge Back</errorMessage>
    </validationRules>
