    <fields>
        <fullName>BillNPay__Status_Icon__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL( BillNPay__Status__c , &quot;Request Sent&quot;),
  IF(TODAY() &lt;= BillNPay__Due_Date__c, IMAGE(&apos;/resource/BillNPay__Pmt_Status_Request_Sent_Green&apos;,&apos;Sent Green&apos;,25,44),
  IF(TODAY() &lt;= BillNPay__Due_Date__c+30, IMAGE(&apos;/resource/BillNPay__Pmt_Status_Request_Sent_Orange&apos;,&apos;Sent Orange&apos;,25,44),
     IMAGE(&apos;/resource/BillNPay__Pmt_Status_Request_Sent_Red&apos;,&apos;Sent Red&apos;,25,44)
  )),
CASE( BillNPay__Status__c ,
&quot;Approved/Paid&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Paid&apos;,&apos;Paid&apos;,25,44),
&quot;Declined&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Decline&apos;,&apos;Declined&apos;,25,44),
&quot;Void/Refund&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Refund&apos;,&apos;Void/Refund&apos;,25,44),
&quot;ACH Returned&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Alert&apos;,&apos;Alert&apos;,25,44),
&quot;Chargeback&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Alert&apos;,&apos;Alert&apos;,25,44),
&quot;Cancelled&quot;,IMAGE(&apos;/resource/BillNPay__Pmt_Status_Cancel&apos;,&apos;Cancelled&apos;,25,44),
IMAGE(&apos;/resource/BillNPay__Pmt_Status_Error&apos;,&apos;Error&apos;,25,44)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
