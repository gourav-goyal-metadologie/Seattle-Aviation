    <fields>
        <fullName>BillNPay__Debit_Day__c</fullName>
        <deprecated>false</deprecated>
        <description>Field not used</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Daily&apos;), &apos;1&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Weekly&apos;), &apos;7&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Monthly&apos;), &apos;30&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Quarterly&apos;), &apos;120&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Annual&apos;), &apos;365&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Bi-Weekly&apos;), &apos;14&apos;,IF(ISPICKVAL(BillNPay__Payment_Frequency__c,&apos;Bi-Monthly&apos;), &apos;60&apos;,null)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Weekly: Day of Week (1=Monday)
Monthly: If 29,30,31, pmt will be last day of month if month has a fewer days
Bi-Weekly: 1st pmt day. Next pmt 2 weeks later
Bi-Monthly: 1st pmt day. Next pmt 14 days later or EOM (Value must be &lt;18)</inlineHelpText>
        <label>Debit Day</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
