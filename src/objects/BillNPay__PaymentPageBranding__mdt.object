    <validationRules>
        <fullName>BillNPay__InvalidURL</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISBLANK(BillNPay__RedirectURL__c)),
    NOT(BEGINS(BillNPay__RedirectURL__c, &quot;https://&quot;)),
    NOT(BEGINS(BillNPay__RedirectURL__c, &quot;http://&quot;))
)</errorConditionFormula>
        <errorDisplayField>BillNPay__RedirectURL__c</errorDisplayField>
        <errorMessage>URL must be start with http or https</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__RedirectUrlAndLabel</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
 AND(
   NOT(ISBLANK(BillNPay__RedirectURL__c)),
   ISBLANK(BillNPay__RedirectURLButtonLabel__c)
 ),
 AND(
   NOT(ISBLANK(BillNPay__RedirectURLButtonLabel__c)),
   ISBLANK(BillNPay__RedirectURL__c)
 )
)</errorConditionFormula>
        <errorMessage>Redirect URL &amp; Redirect URL button Label both must be empty OR both must not empty</errorMessage>
    </validationRules>
