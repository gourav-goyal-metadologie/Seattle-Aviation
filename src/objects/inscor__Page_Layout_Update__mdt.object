    <validationRules>
        <fullName>inscor__Require_Button_Name_on_Type_Button</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( inscor__Change_Type__c , &apos;Button&apos;) , ISBLANK( inscor__Button_Name__c ) )</errorConditionFormula>
        <errorDisplayField>inscor__Button_Name__c</errorDisplayField>
        <errorMessage>Button Name is required when Change Type is Button</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Require_Field_Name_on_Type_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( inscor__Change_Type__c , &apos;Field&apos;) ,  ISBLANK( inscor__Field_Name__c ) )</errorConditionFormula>
        <errorDisplayField>inscor__Field_Name__c</errorDisplayField>
        <errorMessage>Field Name required when Change Type is Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Require_Section_on_Type_Section</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( inscor__Change_Type__c , &apos;Section&apos;) , ISBLANK( inscor__Section__c ) )</errorConditionFormula>
        <errorDisplayField>inscor__Section__c</errorDisplayField>
        <errorMessage>Section is required when Change Type is Section</errorMessage>
    </validationRules>
