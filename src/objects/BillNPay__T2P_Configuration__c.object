    <validationRules>
        <fullName>BillNPay__Activation_must_not_be_in_Test_Mode</fullName>
        <active>true</active>
        <description>When making a Gateway active, do not have Test mode checked</description>
        <errorConditionFormula>PRIORVALUE( BillNPay__Active__c ) = FALSE &amp;&amp;
BillNPay__Active__c = TRUE  &amp;&amp;
BillNPay__Enable_Mock_Gateway__c = TRUE</errorConditionFormula>
        <errorDisplayField>BillNPay__Active__c</errorDisplayField>
        <errorMessage>when making a Gateway Active, the Test Mode must be unchecked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillNPay__Deactivation_requires_mock_gateway_first</fullName>
        <active>true</active>
        <description>Prevents the deactivation of a gateway without first setting the payment method to Test Mode</description>
        <errorConditionFormula>PRIORVALUE( BillNPay__Active__c ) = TRUE &amp;&amp;
BillNPay__Active__c = FALSE &amp;&amp;
BillNPay__Enable_Mock_Gateway__c = FALSE</errorConditionFormula>
        <errorDisplayField>BillNPay__Active__c</errorDisplayField>
        <errorMessage>Before deactivating a Gateway, it must be set to Test Mode first.  That will clear out the Gateway credentials in preparation for a new Gateway to be activated.   Use caution in Production, this will stop payment processing.</errorMessage>
    </validationRules>
