    <validationRules>
        <fullName>inscor__Restrict_For_Out_Of_Date_Calib</fullName>
        <active>true</active>
        <description>Restrict Tools Allocations for out of date calibration.</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(inscor__Date_Checked_Out__c)) &amp;&amp; inscor__Inventory__c != null &amp;&amp; inscor__Inventory__r.inscor__Product__c != null &amp;&amp; 
(inscor__Inventory__r.inscor__Product__r.inscor__Calibration_Required__c == true &amp;&amp; (inscor__Inventory__r.inscor__Next_Calibration_Date__c &lt; TODAY() || ISBLANK(inscor__Inventory__r.inscor__Next_Calibration_Date__c))) ||
(inscor__Inventory__r.inscor__Product__r.inscor__Calibration_Required__c == false &amp;&amp; inscor__Inventory__r.inscor__Next_Calibration_Date__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>Tool cannot be allocated as calibration date has passed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Restrict_For_Out_Of_Date_Insp</fullName>
        <active>true</active>
        <description>Restrict Tools Allocations for out of date inspection.</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(inscor__Date_Checked_Out__c)) &amp;&amp; inscor__Inventory__c != null &amp;&amp; inscor__Inventory__r.inscor__Product__c != null &amp;&amp; 
(inscor__Inventory__r.inscor__Product__r.inscor__Inspection_Required__c == true &amp;&amp; (inscor__Inventory__r.inscor__Next_Inspection_Date__c &lt; TODAY() || ISBLANK(inscor__Inventory__r.inscor__Next_Inspection_Date__c))) ||
(inscor__Inventory__r.inscor__Product__r.inscor__Inspection_Required__c == false &amp;&amp; inscor__Inventory__r.inscor__Next_Inspection_Date__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>Tool cannot be allocated as inspection date has passed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Restrict_For_Out_Of_Date_Insp_Or_Calib</fullName>
        <active>false</active>
        <description>Restrict Tools Allocations for out of date allocations or inspection dates.</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(inscor__Date_Checked_Out__c)) &amp;&amp; inscor__Inventory__c != null &amp;&amp; inscor__Inventory__r.inscor__Product__c != null &amp;&amp; 
((inscor__Inventory__r.inscor__Product__r.inscor__Inspection_Required__c == true &amp;&amp; (inscor__Inventory__r.inscor__Next_Inspection_Date__c &lt; TODAY() || ISBLANK(inscor__Inventory__r.inscor__Next_Inspection_Date__c))) || 
(inscor__Inventory__r.inscor__Product__r.inscor__Calibration_Required__c == true &amp;&amp; (inscor__Inventory__r.inscor__Next_Calibration_Date__c &lt; TODAY() || ISBLANK(inscor__Inventory__r.inscor__Next_Calibration_Date__c))) || 
(inscor__Inventory__r.inscor__Product__r.inscor__Tool__c != true))</errorConditionFormula>
        <errorMessage>Tool cannot be allocated as either inspection or calibration date has passed or &quot;tool?&quot; is not checked on the part master.</errorMessage>
    </validationRules>
