        <active>true</active>
        <errorConditionFormula>AND( 
ISCHANGED(inscor__Status__c), 
TEXT(PRIORVALUE(inscor__Status__c)) = &quot;Cancelled&quot; 
)</errorConditionFormula>
        <errorMessage>Once cancelled, the order cannot be reopened.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cant_change_from_invoiced</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISCHANGED(inscor__Status__c), 
TEXT(PRIORVALUE(inscor__Status__c)) = &quot;Invoiced&quot; 
)</errorConditionFormula>
        <errorMessage>Once invoiced, the Order can&apos;t be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Customer_PO_Required</fullName>
        <active>false</active>
        <description>Require entry of customer PO</description>
        <errorConditionFormula>inscor__Customer_PO_Number__c  = &apos;&apos;</errorConditionFormula>
        <errorMessage>Customer PO is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Dropship_Site_And_Ship_To_Address</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(inscor__Dropship_to_Account__c)) &amp;&amp; NOT(ISBLANK( inscor__Ship_To_Address__c))</errorConditionFormula>
        <errorMessage>Can not set &quot;Dropship to Account&quot; and &quot;Ship To Address&quot; together.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Status_cannot_be_previous</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;In_Progress&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Partially_Allocated&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Allocated&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Shipped&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;New&quot;)
   )
,
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Partially_Allocated&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Allocated&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Shipped&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;In_Progress&quot;)
   )
,
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Allocated&quot;),
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Shipped&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;Partially_Allocated&quot;)
   )
,
   AND
   (
     OR(
       ISPICKVAL(PRIORVALUE( inscor__Status__c ), &quot;Shipped&quot;)
       ),
      ISPICKVAL(inscor__Status__c, &quot;Allocated&quot;)
   )
)</errorConditionFormula>
        <errorMessage>Status cannot be changed to a previous status.</errorMessage>
    </validationRules>
