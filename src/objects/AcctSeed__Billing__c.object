    <fields>
        <fullName>AcctSeed__Age__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(AND(TODAY() - AcctSeed__Due_Date2__c &lt;= 0,(ROUND(AcctSeed__Balance__c, 2) &gt; 0)), &quot;0-Current&quot;,
IF(AND(30 &gt;= TODAY() - AcctSeed__Due_Date2__c, TODAY() - AcctSeed__Due_Date2__c &gt; 0, (ROUND(AcctSeed__Balance__c, 2) &gt; 0)), &quot;1-30 Days&quot;,
IF(AND(60 &gt;= TODAY() - AcctSeed__Due_Date2__c, TODAY() - AcctSeed__Due_Date2__c &gt; 0, (ROUND(AcctSeed__Balance__c, 2) &gt; 0)), &quot;31-60 Days&quot;,
IF(AND(90 &gt;= TODAY() - AcctSeed__Due_Date2__c, TODAY() - AcctSeed__Due_Date2__c &gt; 0, (ROUND(AcctSeed__Balance__c, 2) &gt; 0)), &quot;61-90 Days&quot;,
IF(AND(TODAY() - AcctSeed__Due_Date2__c &gt; 90, (ROUND(AcctSeed__Balance__c, 2) &gt; 0)), &quot;Over 90 Days&quot;,
