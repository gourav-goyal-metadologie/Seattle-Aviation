    <validationRules>
        <fullName>inscor__Cancelled_Status_Restriction</fullName>
        <active>true</active>
        <errorConditionFormula>AND(inscor__Quantity_Allocated__c +  inscor__Quantity_Shipped__c + inscor__Quantity_Invoiced__c &gt; 0, ISPICKVAL( inscor__Status__c , &apos;Cancelled&apos;) )</errorConditionFormula>
        <errorMessage>You can not change status to cancelled, if sum of Quantity Allocated, Quantity Shipped and Quantity Invoiced is greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cannot_change_CC_once_allocated</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISCHANGED( inscor__Condition_Code__c ),  inscor__Quantity_Allocated__c &gt; 0)</errorConditionFormula>
        <errorMessage>Cannot change Condition Code once allocated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cannot_change_Part_Number_once_allocated</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED(  inscor__Product__c  ),  inscor__Quantity_Allocated__c &gt; 0)</errorConditionFormula>
        <errorMessage>Cannot change Part Number once allocated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cannot_change_quantity_less_than_alloc</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED(   inscor__Quantity_Ordered__c   ),  inscor__Quantity_Allocated__c &gt;  inscor__Quantity_Ordered__c )</errorConditionFormula>
        <errorMessage>Cannot change quantity ordered to less than allocated amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Check_Quantity_on_Update</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
inscor__Quantity_Ordered__c &lt; ( inscor__Quantity_Shipped__c + inscor__Quantity_Allocated__c), NOT(ISPICKVAL(inscor__Order_Type__c , &apos;Credit&apos; )))</errorConditionFormula>
        <errorMessage>Quantity Ordered cannot be less than Quantity Shipped + Quantity Allocated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Closed_Status_Restriction</fullName>
        <active>true</active>
        <errorConditionFormula>AND(inscor__Quantity_Ordered__c != inscor__Quantity_Shipped__c, ISPICKVAL( inscor__Status__c , &apos;Closed&apos;) )</errorConditionFormula>
        <errorMessage>You can only set status to closed when Quantity Ordered = Quantity Shipped</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Exchange_fields_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( inscor__Order_Type__c , &apos;Exchange&apos;) , OR( ISPICKVAL(inscor__Exchange_Type__c , &quot;&quot;), ISBLANK(inscor__Outright_Price__c), ISBLANK(inscor__Core_Due__c), AND(NOT(ISNEW()),ISPICKVAL(inscor__Core_Disposition__c , &quot;&quot;)),AND(NOT(ISNEW()),ISPICKVAL(inscor__Stock_Disposition__c , &quot;&quot;)) ))</errorConditionFormula>
        <errorMessage>For Exchange orders, Outright price, Core Due, Core Disposition, and Stock Disposition are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Part_Number_Required</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Product__c == null</errorConditionFormula>
        <errorMessage>Part Number is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Prevent_Changing_Order_Type_Once_Shipped</fullName>
        <active>true</active>
        <errorConditionFormula>AND(inscor__Quantity_Shipped__c &gt; 0, ISCHANGED( inscor__Order_Type__c ))</errorConditionFormula>
        <errorMessage>Can&apos;t change Order Type once line is Shipped.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Quantity_Check</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Quantity_Ordered__c &lt; 0</errorConditionFormula>
        <errorMessage>Negative values not allowed for Quantity Ordered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Restrict_Sales_by_Condition_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c)), OR(
INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c, &apos;All&apos;), 
AND(inscor__Condition_Code__r.Name == &apos;AR&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;AR&apos;))), 
AND(inscor__Condition_Code__r.Name == &apos;NS&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;NS&apos;))),
AND(inscor__Condition_Code__r.Name == &apos;FN&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;FN&apos;))),
AND(inscor__Condition_Code__r.Name == &apos;OH&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;OH&apos;))),
AND(inscor__Condition_Code__r.Name == &apos;NE&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;NE&apos;))),
AND(inscor__Condition_Code__r.Name == &apos;BER&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;BER&apos;))),
AND(inscor__Condition_Code__r.Name == &apos;SV&apos;, NOT(INCLUDES( inscor__Product__r.inscor__Restrict_Sales_to_Condition_Code__c,&apos;SV&apos;)))))</errorConditionFormula>
        <errorMessage>Part Number is restricted from sale in the selected condition code.</errorMessage>
    </validationRules>
