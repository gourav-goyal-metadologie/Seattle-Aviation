    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>inscor__Condition_Code_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Non_Inventory_Item__c == false &amp;&amp; ISBLANK( inscor__Condition_Code__c)</errorConditionFormula>
        <errorDisplayField>inscor__Condition_Code__c</errorDisplayField>
        <errorMessage>Condition Code is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Cons_Owner_for_Cons_Purchases</fullName>
        <active>true</active>
        <description>Check that Owner Codes used on Consignment POs are Consignment Owners</description>
        <errorConditionFormula>AND (inscor__Owner_Code__r.inscor__Consignment__c &lt;&gt; True, 
(ISPICKVAL(inscor__PO_Number__r.inscor__PO_Type__c , &apos;Consignment&apos;)))</errorConditionFormula>
        <errorMessage>You must use a Consignment Owner Code when PO Header Type = Consignment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Consignment_lines_cannot_have_price</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  inscor__Owner_Code__r.inscor__Consignment__c  = true,  inscor__Price__c &gt;0)</errorConditionFormula>
        <errorMessage>PO Lines for consignment owner codes should be $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Expected_delivery_cannot_be_previous</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Expected_Delivery__c &lt;  TODAY()</errorConditionFormula>
        <errorMessage>Expected Delivery date cannot be prior to today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Initial_Promise_date_cannot_be_previous</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Initial_Promise_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorMessage>Initial promise date cannot be prior to today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__LOT_Owner_for_LOT_Purchases</fullName>
        <active>true</active>
        <description>Check that Owner Codes used on LOT POs are LOT Owners</description>
        <errorConditionFormula>AND (inscor__Owner_Code__r.inscor__Lot_Purchase__c  &lt;&gt; True, 
(ISPICKVAL(inscor__PO_Number__r.inscor__PO_Type__c , &apos;Lot&apos;)))</errorConditionFormula>
        <errorMessage>You must use a LOT Owner Code when PO Header Type = LOT.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Lot_lines_cannot_have_price</fullName>
        <active>true</active>
        <description>No Longer Needed.</description>
        <errorConditionFormula>AND( inscor__Owner_Code__r.inscor__Lot_Purchase__c = true,  inscor__Price__c &gt;0)</errorConditionFormula>
        <errorMessage>PO Lines for Lot owner codes should be $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Non_Cons_Owner_for_Non_Cons_PO_Header</fullName>
        <active>true</active>
        <description>Check to ensure Consignment owners are not used on non Consignment PO Headers</description>
        <errorConditionFormula>AND(inscor__Owner_Code__r.inscor__Consignment__c = True, 
NOT(ISPICKVAL(inscor__PO_Number__r.inscor__PO_Type__c , &apos;Consignment&apos;)))</errorConditionFormula>
        <errorMessage>You cannot use a Consignment Owner Code when PO Header is not a Consignment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Non_LOT_Owner_for_Non_LOT_PO_Header</fullName>
        <active>true</active>
        <description>Check to ensure LOT owners are not used on non LOT PO Headers</description>
        <errorConditionFormula>AND(inscor__Owner_Code__r.inscor__Lot_Purchase__c  = True, 
NOT(ISPICKVAL(inscor__PO_Number__r.inscor__PO_Type__c , &apos;Lot&apos;)))</errorConditionFormula>
        <errorMessage>You cannot use a LOT Owner Code when PO Header is not a LOT.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Owner_Code_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW() = FALSE  &amp;&amp;  ISBLANK( inscor__Owner_Code__c )</errorConditionFormula>
        <errorDisplayField>inscor__Owner_Code__c</errorDisplayField>
        <errorMessage>Owner Code is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Owner_Code_or_Non_Inventory_Part_Require</fullName>
        <active>true</active>
        <errorConditionFormula>inscor__Product__r.inscor__Non_Inventory_Item__c == false &amp;&amp;   ISBLANK(inscor__Owner_Code__c)</errorConditionFormula>
        <errorMessage>Owner code missing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Part_Number_Required</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Product__c == null</errorConditionFormula>
        <errorMessage>Part Number is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Partial_Receive_Price_Change</fullName>
        <active>true</active>
        <description>Do not allow price changes on PO lines in partially received status.</description>
        <errorConditionFormula>AND(ISPICKVAL( inscor__Status__c , &apos;Sent To Vendor&apos;),  inscor__Quantity_Received__c &gt; 0, 
inscor__Quantity_Received__c &lt; inscor__Quantity__c, 
ISCHANGED( inscor__Price__c ))</errorConditionFormula>
        <errorMessage>Price change not allowed due to partially rec&apos;d status. Contact your Accounts Payable Dept.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Price_Cannot_Change_After_Paid</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(inscor__Price__c) &amp;&amp;  NOT( ISCHANGED(inscor__Quantity_Paid__c))  &amp;&amp;  inscor__Quantity__c ==  inscor__Quantity_Paid__c</errorConditionFormula>
        <errorDisplayField>inscor__Price__c</errorDisplayField>
        <errorMessage>Price can not change after paid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Price_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() = FALSE  &amp;&amp;  ISBLANK( inscor__Price__c )  &amp;&amp;  (inscor__Owner_Code__r.inscor__Consignment__c = FALSE &amp;&amp;  inscor__Owner_Code__r.inscor__Lot_Purchase__c = FALSE)</errorConditionFormula>
        <errorDisplayField>inscor__Price__c</errorDisplayField>
        <errorMessage>Price cannot be left blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Qty_Ordered_less_than_Qty_Received</fullName>
        <active>true</active>
        <description>Validates the qty amount that is allowed.</description>
        <errorConditionFormula>IF(
OR(ISPICKVAL(inscor__Type_Quoted__c,&apos;Return Inventory&apos;),ISPICKVAL(inscor__Type_Quoted__c, &apos;Return EX Core&apos;)),
false,
inscor__Quantity__c  &lt;  ( BLANKVALUE(inscor__Quantity_Received__c, 0) + BLANKVALUE(inscor__Quantity_Allocated__c,0) )
)</errorConditionFormula>
        <errorDisplayField>inscor__Quantity__c</errorDisplayField>
        <errorMessage>Qty Ordered cannot be less than Qty Received plus Qty Allocated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Quantity_Check</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Quantity__c &lt; 0</errorConditionFormula>
        <errorMessage>Negative values not allowed for Prices and Quantity fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Restrict_Shelf_Life_Updation</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED(inscor__Shelf_Life__c)</errorConditionFormula>
        <errorDisplayField>inscor__Shelf_Life__c</errorDisplayField>
        <errorMessage>cannot change Shelf life, it will be copied from Part Number !!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Type_Purchase_Required</fullName>
        <active>true</active>
        <description>Makes the Type Purchase field required for PO Lines</description>
        <errorConditionFormula>ISBLANK( TEXT(inscor__Type_Quoted__c) )</errorConditionFormula>
        <errorMessage>Type Purchase is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>inscor__Warehouse_Code_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>inscor__Product__r.inscor__Non_Inventory_Item__c == false &amp;&amp; ISBLANK(  inscor__Warehouse__c  )</errorConditionFormula>
        <errorDisplayField>inscor__Warehouse__c</errorDisplayField>
        <errorMessage>Warehouse Code is required</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
